Grammar

    0 $accept: program $end

    1 program: %empty
    2        | program funcdef ';'

    3 funcdef: ID '(' maybepars ')' maybestats END

    4 maybepars: %empty
    5          | pars

    6 pars: ID
    7     | pars ',' ID

    8 maybestats: %empty
    9           | stats

   10 stats: stat ';'
   11      | stats stat ';'

   12 stat: RETURN expr
   13     | cond
   14     | VAR ID '=' expr
   15     | ID '=' expr

   16 cond: maybelabeldef COND maybeguards END

   17 maybelabeldef: %empty
   18              | ID ':'

   19 maybeguards: %empty
   20            | guards

   21 guards: guarded ';'
   22       | guards guarded ';'

   23 guarded: guard stats control

   24 guard: expr GUARD
   25      | GUARD

   26 control: CONTINUE
   27        | CONTINUE ID
   28        | BREAK
   29        | BREAK ID

   30 expr: term
   31     | nhtis
   32     | plusterms
   33     | multterms
   34     | orterms
   35     | dotterms
   36     | term gteqeqminus term

   37 nhtis: nhti term
   38      | nhtis nhti

   39 nhti: NOT
   40     | HEAD
   41     | TAIL
   42     | ISLIST

   43 plusterms: term '+' term
   44          | plusterms '+' term

   45 multterms: term '*' term
   46          | multterms '*' term

   47 orterms: term OR term
   48        | orterms OR term

   49 dotterms: term '.' term
   50         | dotterms '.' term

   51 gteqeqminus: GREQ
   52            | '='
   53            | '-'

   54 term: '(' expr ')'
   55     | NUM
   56     | ID
   57     | ID '(' maybeparams ')'

   58 maybeparams: %empty
   59            | params

   60 params: expr
   61       | params ',' expr


